{
  "name": "medopd-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^20.11.1",
    "npm": "^10.2.4"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch | pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "cz",
    "migrate:generate": "drizzle-kit generate:pg --config=./src/migrations/migrate.config.ts",
    "drizzle-studio": "drizzle-kit studio --config=./src/migrations/migrate.config.ts",
    "migrate:apply": "node dist/migrations/migrate",
    "seed": "node dist/seed/seed run",
    "generate:graphql:schema": "node dist/graphql-schema/graphql-schema",
    "publish:schema": "npm run generate:graphql:schema"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@aws-sdk/client-s3": "^3.550.0",
    "@aws-sdk/lib-storage": "^3.550.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/websockets": "^10.3.7",
    "@onesignal/node-onesignal": "^1.0.0-beta4",
    "axios": "^1.6.8",
    "axios-logger": "^2.8.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.30.5",
    "ejs": "^3.1.10",
    "file-type": "^16.5.3",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "livekit-server-sdk": "^1.2.3",
    "nanoid": "^3.3.4",
    "nest-commander": "^3.12.5",
    "nestjs-pino": "^4.0.0",
    "node-html-to-image": "^4.0.0",
    "otplib": "^12.0.1",
    "passport-jwt": "^4.0.1",
    "postgres": "^3.4.3",
    "puppeteer": "^22.3.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.13",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
